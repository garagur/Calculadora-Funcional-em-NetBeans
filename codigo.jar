private void calcularResultado() { // essa é a funcao que faz a transformacao da string para uma operacao logica e a executa, tambem tem um tratamento de erro para casos que sao impossiveis de calcular como divisao por 0
    try {
        Expression e = new ExpressionBuilder(entrada).build();
        double resultado = e.evaluate();
        entrada = Double.toString(resultado);
        saida.setText(entrada);
    } catch (Exception ex) {
        saida.setText("Sintaxe Erro");
    }
}
                  
